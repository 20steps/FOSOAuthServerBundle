<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="fos.oauth_server.authentication.factory.class">FOS\OAuthServerBundle\DependencyInjection\Security\Factory\OAuthFactory</parameter>
    </parameters>

    <services>
        <service id="security.authentication.factory.fos_oauth2_server"
            class="%fos.oauth_server.authentication.factory.class%" public="false">
            <tag name="security.listener.factory" />
        </service>
    </services>
</container>
